@model IEnumerable<WebApplication3.Models.Employee>

@{
}
<a asp-action="Create" asp-controller="Employee" class="btn btn-success">Create</a>
<h1>Employee Record</h1>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Gender</th>
        <th>Email</th>
        <th>Address</th>
        <th>Actoin</th>
    </tr>
    @foreach (var emp in Model)
    {
        <tr>
            <td>@emp.Id</td>
            <td>@emp.Name</td>
            <td>@emp.Gender</td>
            <td>@emp.Email</td>
            <td>@emp.Address</td>
            <td>
                <a asp-action="Edit" asp-controller="Employee" asp-route-id="@emp.Id">Edit</a>||
                <a asp-action="Delete" asp-controller="Employee" asp-route-id="@emp.Id">Delete</a>
            </td>
        </tr>
    }

</table>